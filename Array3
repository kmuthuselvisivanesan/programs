// Program to access elements by providing input

package learning;

import java.util.Scanner;

public class Array3 {

	public static void main(String[] args) {
          int a[]=new int[10]; //memory allocation
        		  for(int i=0;i<=9;i++)
        		     {
        			    System.out.println("enter the element");
        			    Scanner sc = new Scanner(System.in);
        			    a[i]=sc.nextInt();
        			  
        		      }
        		  for(int elements:a)
        		      {
        		         System.out.println(elements);
        		      } 

	}

}


output:
enter the element
1
enter the element
2
enter the element
